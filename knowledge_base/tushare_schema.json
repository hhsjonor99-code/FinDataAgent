[
  {
    "function_name": "pro.daily",
    "description": "获取A股每日行情数据，涵盖开盘、收盘、最高、最低价，以及成交量、成交额等关键指标。支持单只或多只股票查询。",
    "parameters": [
      {
        "name": "ts_code",
        "type": "str",
        "required": false,
        "description": "股票代码，支持多个股票同时提取，用逗号分隔。例如 '000001.SZ' 或 '000001.SZ,600000.SH'。"
      },
      {
        "name": "trade_date",
        "type": "str",
        "required": false,
        "description": "交易日期（YYYYMMDD格式）。单独使用可获取指定日期的所有股票行情。与start_date/end_date任选其一。"
      },
      {
        "name": "start_date",
        "type": "str",
        "required": false,
        "description": "开始日期（YYYYMMDD格式）。"
      },
      {
        "name": "end_date",
        "type": "str",
        "required": false,
        "description": "结束日期（YYYYMMDD格式）。"
      }
    ],
    "output_columns": [
      {"name": "ts_code", "type": "str", "description": "股票代码"},
      {"name": "trade_date", "type": "str", "description": "交易日期"},
      {"name": "open", "type": "float", "description": "开盘价"},
      {"name": "high", "type": "float", "description": "最高价"},
      {"name": "low", "type": "float", "description": "最低价"},
      {"name": "close", "type": "float", "description": "收盘价"},
      {"name": "pre_close", "type": "float", "description": "昨收价(前复权)"},
      {"name": "change", "type": "float", "description": "涨跌额"},
      {"name": "pct_chg", "type": "float", "description": "涨跌幅(%)"},
      {"name": "vol", "type": "float", "description": "成交量(手)"},
      {"name": "amount", "type": "float", "description": "成交额(千元)"}
    ],
    "required_imports": ["import tushare as ts"],
    "example": "pro = ts.pro_api()\ndf = pro.daily(ts_code='000001.SZ', start_date='20180701', end_date='20180718')\n# 获取单日全部股票行情\ndf_all = pro.daily(trade_date='20180810')",
    "time_field": "trade_date",
    "default_visual": {"x_col": "trade_date", "y_col": "close"}
  },
  {
    "function_name": "pro.cn_gdp",
